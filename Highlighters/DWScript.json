            }
          },
          {
            "OpenToken": "INTERFACE",
            "CloseToken": "IMPLEMENTATION",
            "Properties": {
              "OpenTokenBeginningOfLine": true,
              "TokenEndIsPreviousLine": true,
              "ShowGuideLine": false
