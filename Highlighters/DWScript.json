  "CodeFolding": {
    "Ranges": [
      {
        "Options": {
          "EscapeChar": "&"
        },
        "SkipRegion": [
          {
            "OpenToken": "'",
            "CloseToken": "'",
            "RegionType": "SingleLineString"
          },
          {
            "OpenToken": "//",
            "RegionType": "SingleLine"
          },
          {
            "OpenToken": "{",
            "CloseToken": "}",
            "RegionType": "MultiLine",
            "NextCharIsNot": "$"
          },
          {
            "OpenToken": "(*",
            "CloseToken": "*)",
            "RegionType": "MultiLine"
          }
        ],
        "FoldRegion": [
          {
            "OpenToken": "BEGIN",
            "CloseToken": "END"
          },
          {
            "OpenToken": "REPEAT",
            "CloseToken": "UNTIL"
          },
          {
            "OpenToken": "TRY",
            "CloseToken": "END",
