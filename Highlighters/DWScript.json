          },
          {
            "OpenToken": "INTERFACE",
            "CloseToken": "END",
            "Properties": {
              "NoSubs": true,
              "SkipIfFoundAfterOpenToken": [
                ";"
              ]
            }
          },
          {
            "OpenToken": "IMPLEMENTATION",
            "CloseToken": "END.",
            "Properties": {
              "OpenTokenBeginningOfLine": true,
              "OpenIsClose": true,
              "SharedClose": true,
              "ShowGuideLine": false
            }
          },
          {
            "OpenToken": "UNIT",
            "CloseToken": "END.",
            "Properties": {
              "OpenTokenBeginningOfLine": true,
              "SharedClose": true
            }
          },
          {
            "OpenToken": "PROGRAM",
            "CloseToken": "END.",
            "Properties": {
              "OpenTokenBeginningOfLine": true,
              "SharedClose": true
            }
          },
          {
            "OpenToken": "$REGION",
