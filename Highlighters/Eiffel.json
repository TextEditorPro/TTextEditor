{
  "Highlighter": {
    "Sample": [
      "-- Hello, World! in Eiffel\r\n",
      "\r\n",
      "class\r\n",
      "    HELLO_WORLD\r\n",
      "create\r\n",
      "    make\r\n",
      "feature\r\n",
      "    make\r\n",
      "        do\r\n",
      "            print (\"Hello, World!%N\")\r\n",
      "        end\r\n",
      "end"
    ],
    "MainRules": {
      "Attributes": {
