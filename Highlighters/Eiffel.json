      },
      "SubRules": {
        "Range": [
          {
            "Type": "LineComment",
            "Attributes": {
              "Element": "Comment"
            },
            "Properties": {
              "CloseOnEndOfLine": true
            },
            "TokenRange": {
              "Open": "--"
            }
          },
          {
            "Type": "Method",
            "Attributes": {
              "Element": "MethodName"
            },
            "Properties": {
              "CloseOnEndOfLine": true,
              "CloseOnTerm": true,
              "SkipWhitespace": true
            },
            "TokenRange": {
              "Open": ":",
              "Close": "like"
            },
            "SubRules": {
              "KeyList": [
                {
                  "Type": "ReservedWord",
                  "Words": [
                    "like"
                  ],
                  "Attributes": {
                    "Element": "ReservedWord"
                  }
                }
              ]
            }
          },
          {
            "Type": "Method",
            "Attributes": {
              "Element": "MethodName"
            },
            "TokenRange": {
              "Open": "{",
              "Close": "}"
