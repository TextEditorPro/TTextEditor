  "CodeFolding": {
    "Ranges": [
      {
        "Options": {
          "StringEscapeChar": "\\"
        },
        "SkipRegion": [
          {
            "OpenToken": "\"",
            "CloseToken": "\"",
            "RegionType": "SingleLineString"
          },
          {
            "OpenToken": "'",
            "RegionType": "SingleLine"
          },
          {
            "OpenToken": "REM",
            "RegionType": "SingleLine"
          },
          {
            "OpenToken": "/*",
            "CloseToken": "*/",
            "RegionType": "MultiLine"
          }
        ],
        "FoldRegion": [
          {
            "OpenToken": "SUB",
            "CloseToken": "END SUB",
            "Properties": {
              "OpenTokenBeginningOfLine": true,
              "BreakCharFollows": false
            }
          },
          {
            "OpenToken": "FUNCTION",
            "CloseToken": "END FUNCTION",
            "Properties": {
              "OpenTokenBeginningOfLine": true,
              "BreakCharFollows": false
            }
          }
        ]
      }
    ]
  },
  "MatchingPair": {
