              "exp",
              "floor",
              "gcf",
              "get",
              "isdone",
              "keys",
              "length",
              "lcm",
              "log",
              "log10",
              "logx",
              "loop",
              "max",
              "min",
              "pow",
              "put",
              "random",
              "round",
              "set",
              "sin",
              "sqrt",
              "tan",
              "unbind",
              "xrt",
              "abs",
              "exit",
              "foo",
              "nanotime",
              "print",
              "write",
              "yield",
              "yieldWaitTime"
            ],
            "Attributes": {
              "Element": "Method"
            }
          },
          {
            "Type": "String",
            "Words": [
              "bool",
              "console",
              "env",
              "fiber",
              "float",
              "int",
              "math",
              "string",
              "system"
            ],
            "Attributes": {
              "Element": "String"
            }
          },
          {
            "Type": "Symbol",
            "Words": [
              "e",
              "ln10",
              "ln2",
              "log10e",
              "log2e",
              "pi",
              "sqrt1_2",
              "sqrt2"
            ],
            "Attributes": {
              "Element": "Symbol"
            }
          },
          {
            "Type": "Symbol",
            "Words": [
              ":",
              ";",
              "<",
              ">",
              "=",
              "/",
              "*",
              ".",
              ",",
              "-",
              "+",
              "(",
              ")",
              "[",
              "]",
              "{",
              "}",
              "~",
              "^",
              "%",
              "!",
              "&",
              "|"
            ],
            "Attributes": {
              "Element": "Symbol"
            }
          }
        ],
        "Set": [
          {
            "Type": "Numbers",
            "Symbols": ".0123456789",
            "Attributes": {
              "Element": "Number"
            }
          }
        ]
      }
    }
  },
  "CodeFolding": {
    "Ranges": [
      {
        "Options": {
          "StringEscapeChar": "\\"
        },
        "SkipRegion": [
          {
            "OpenToken": "'",
            "CloseToken": "'",
            "RegionType": "SingleLineString"
          },
          {
            "OpenToken": "\"",
            "CloseToken": "\"",
            "RegionType": "SingleLineString"
          },
          {
            "OpenToken": "//",
            "RegionType": "SingleLine"
          },
          {
            "OpenToken": "/*",
            "CloseToken": "*/",
            "RegionType": "MultiLine"
          }
        ],
        "FoldRegion": [
          {
            "OpenToken": "{",
            "CloseToken": "}",
            "Properties": {
              "BreakCharFollows": false
            }
          }
        ]
      }
    ]
  },
  "MatchingPair": {
    "Pairs": [
      {
        "OpenToken": "'",
        "CloseToken": "'"
      },
      {
        "OpenToken": "\"",
        "CloseToken": "\""
      },
      {
        "OpenToken": "(",
        "CloseToken": ")"
      },
      {
        "OpenToken": "[",
        "CloseToken": "]"
      },
      {
        "OpenToken": "{",
        "CloseToken": "}"
      }
    ]
  },
  "CompletionProposal": {
    "SkipRegion": [
      {
        "OpenToken": "'",
        "CloseToken": "'",
        "RegionType": "SingleLineString"
      },
      {
        "OpenToken": "\"",
        "CloseToken": "\"",
        "RegionType": "SingleLineString"
      },
      {
        "OpenToken": "//",
        "RegionType": "SingleLine"
      },
      {
        "OpenToken": "/*",
        "CloseToken": "*/",
        "RegionType": "MultiLine"
      }
    ]
  }
}