  "CodeFolding": {
    "Ranges": [
      {
        "Options": {
          "MatchingPairHighlight": false
        },
        "SkipRegion": [
          {
            "OpenToken": "'",
            "CloseToken": "'",
            "RegionType": "SingleLineString"
          },
          {
            "OpenToken": "\"",
            "CloseToken": "\"",
            "RegionType": "SingleLineString"
          },
          {
            "OpenToken": ";",
            "RegionType": "SingleLine"
          }
        ],
        "FoldRegion": [
          {
            "OpenToken": "[",
            "CloseToken": "[",
            "Properties": {
              "OpenTokenBeginningOfLine": true,
              "CloseTokenBeginningOfLine": true,
              "OpenIsClose": true,
              "TokenEndIsPreviousLine": true,
              "OpenTokenEnd": "]",
              "BreakCharFollows": false
            }
          }
        ]
      }
    ]
