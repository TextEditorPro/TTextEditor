{
  "Highlighter": {
    "Sample": [
      "// Hello, World! in Object-C++\r\n",
      "\r\n",
      "#import <Foundation/Foundation.h>\r\n",
      "#include <print>\r\n",
      "\r\n",
      "int main(int argc, const char * argv[]) {\r\n",
      "    @autoreleasepool {\r\n",
      "        NSLog(@\"Hello, World!\");\r\n",
      "        std::println(\"Hello, C++ world!\");\r\n",
      "    }\r\n",
      "    return 0;\r\n",
      "}"
    ],
