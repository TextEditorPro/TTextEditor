          },
          {
            "OpenToken": "/*",
            "CloseToken": "*/",
            "RegionType": "MultiLine"
          }
        ],
        "FoldRegion": [
          {
            "OpenToken": "[",
            "CloseToken": "]",
            "Properties": {
              "BreakCharFollows": false
            }
          }
        ]
      }
    ]
