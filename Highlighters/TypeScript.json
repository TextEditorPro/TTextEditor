      "// // Hello, World! in TypeScript\r\n",
      "class Greeter {\r\n",
      "	    constructor(public greeting: string) { }\r\n",
      "	    greet() {\r\n",
      "	        return \"<h1>\" + this.greeting + \"</h1>\";\r\n",
      "	    }\r\n",
      "};\r\n",
      "\r\n",
      "var greeter = new Greeter(\"Hello, World!\");\r\n",
      "\r\n",
      "document.body.innerHTML = greeter.greet();"
